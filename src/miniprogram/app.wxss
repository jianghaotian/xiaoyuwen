.overflow_x {
  width: 750rpx;
  overflow-x: hidden;
  box-sizing: border-box;
}
.container {
  height: 100%;
  display: flex;
  box-sizing: border-box;
} 
/* top bar */
.topbar {
  position: fixed;
  top: 0;
  height: 70rpx;
  width: 100%;
  padding-left: 40rpx;
  padding-bottom: 10rpx;
  line-height: 70rpx;
  font-size: 40rpx;
  background-color: #F4E7D7;
  z-index: 99;
}
.placeholder {
  height: 80rpx;
  width: 750rpx;
  margin-bottom: 20rpx;
  z-index: -99;
  opacity: 0;
}
.topbar .small {
  margin-left: 26rpx;
  font-size: 30rpx;
}
/* 字母表格样式 */
.zimu {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 10rpx;
  transform: translateX(3%);
  box-sizing: border-box;
}
.zimu_block {
  display: flex;
  justify-content: center;
  align-items: center;
  /* 每个表格的宽高 */
  width: 150rpx;
  height: 150rpx;
  /* 每个表格的间距 */
  margin: 13.4rpx;
  border: 1rpx solid rgba(0, 0, 0, .2);
  border-radius: 10rpx;
  box-shadow: 0rpx 6rpx 6rpx 6rpx rgba(0, 0, 0, .1);
}
.zimu_block text {
  line-height: 0rpx;
  font-size: 66rpx;
}
