/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  box-sizing: border-box;
} 
/* 字母表格样式 */
.zimu {
  display: flex;
  flex-wrap: wrap;
  /* 整体表格的间距 */
  margin: 32rpx;
  box-sizing: border-box;
}
.zimu_block {
  display: flex;
  align-items: center;
  justify-content: center;
  /* 每个表格的宽高 */
  width: 150rpx;
  height: 150rpx;
  /* 每个表哥的间距 */
  margin: 12rpx;
  border: 1rpx solid rgba(0, 0, 0, .2);
  border-radius: 10rpx;
  box-shadow: 0rpx 6rpx 6rpx 6rpx rgba(0, 0, 0, .1);
}
.zimu_block text {
  line-height: 10rpx;
  font-size: 66rpx;

}