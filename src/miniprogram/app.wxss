
.container {
  height: 100%;
  display: flex;
  box-sizing: border-box;
} 
/* top bar */
.topbar {
  height: 70rpx;
  width: 100%;
  padding-left: 40rpx;
  margin-bottom: 20rpx;
  padding-bottom: 10rpx;
  line-height: 70rpx;
  font-size: 40rpx;
  background-color: #F4E7D7;
}
.topbar .small {
  margin-left: 26rpx;
  font-size: 30rpx;
}
/* 字母表格样式 */
.zimu {
  display: flex;
  flex-wrap: wrap;
  /* 整体表格的间距 */
  /* margin: 0 auto; */
  /* width: 720rpx; */
  transform: translateX(3%);
  box-sizing: border-box;
}
.zimu_block {
  display: flex;
  justify-content: center;
  align-items: center;
  /* 每个表格的宽高 */
  width: 150rpx;
  height: 150rpx;
  /* 每个表格的间距 */
  margin: 13.4rpx;
  border: 1rpx solid rgba(0, 0, 0, .2);
  border-radius: 10rpx;
  box-shadow: 0rpx 6rpx 6rpx 6rpx rgba(0, 0, 0, .1);
}
.zimu_block text {
  line-height: 0rpx;
  font-size: 66rpx;
}

/* 图标 */
@font-face {font-family: "iconfont";
  src: url('https://at.alicdn.com/t/font_1745402_r3rhqfdu4sb.eot?t=1586482425136'); /* IE9 */
  src: url('https://at.alicdn.com/t/font_1745402_r3rhqfdu4sb.eot?t=1586482425136#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('https://at.alicdn.com/t/font_1745402_r3rhqfdu4sb.woff?t=1586482425136') format('woff'),
  url('https://at.alicdn.com/t/font_1745402_r3rhqfdu4sb.ttf?t=1586482425136') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('https://at.alicdn.com/t/font_1745402_r3rhqfdu4sb.svg?t=1586482425136#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-shoucang:before {
  content: "\e600";
}
